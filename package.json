  "license": "MIT",
  "engines": {
    "node": ">= 20.1.0"
  },
  "dependencies": {
    "@types/serve-static": "^1.15.7",
    "debug": "~2||~3||~4",
    "parseurl": "~1",
    "send": "~0",
    "serve-static": "~1"
  },
  "devDependencies": {
    "@figliolia/ts-packager": "^1.2.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.7.4",
    "@types/parseurl": "^1.3.3",
    "@types/send": "^0.17.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "connect": ">=2",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
